{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/TestResult.json",
  "title": "TestResult",
  "description": "Result from test execution.",
  "version": "1.0",
  "additionalProperties": false,
  "properties": {
    "test_name": {
      "title": "Test Name",
      "type": "string"
    },
    "status": {
      "title": "Status",
      "type": "string"
    },
    "duration_seconds": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Duration Seconds"
    },
    "error_message": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Error Message"
    },
    "stdout": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Stdout"
    },
    "stderr": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Stderr"
    }
  },
  "required": [
    "test_name",
    "status"
  ],
  "type": "object"
}