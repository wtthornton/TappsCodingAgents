{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/CodeArtifact.json",
  "title": "CodeArtifact",
  "description": "Versioned code generation artifact.\n\nSchema version: 1.0\nMigrated to Pydantic BaseModel for runtime validation and type safety.",
  "version": "1.0",
  "$defs": {
    "ArtifactMetadata": {
      "additionalProperties": false,
      "description": "Flexible but typed metadata structure for artifacts.\n\nUses TypedDict with total=False to allow partial metadata while maintaining\ntype hints for known fields.",
      "properties": {
        "agent_version": {
          "title": "Agent Version",
          "type": "string"
        },
        "execution_context": {
          "title": "Execution Context",
          "type": "string"
        },
        "custom_tags": {
          "items": {
            "type": "string"
          },
          "title": "Custom Tags",
          "type": "array"
        },
        "related_artifacts": {
          "items": {
            "type": "string"
          },
          "title": "Related Artifacts",
          "type": "array"
        },
        "workflow_id": {
          "title": "Workflow Id",
          "type": "string"
        },
        "step_id": {
          "title": "Step Id",
          "type": "string"
        },
        "agent_id": {
          "title": "Agent Id",
          "type": "string"
        },
        "execution_mode": {
          "title": "Execution Mode",
          "type": "string"
        }
      },
      "title": "ArtifactMetadata",
      "type": "object"
    },
    "ArtifactStatus": {
      "description": "Status of an artifact during workflow execution.",
      "enum": [
        "pending",
        "running",
        "completed",
        "failed",
        "cancelled",
        "timeout",
        "not_run"
      ],
      "title": "ArtifactStatus",
      "type": "string"
    },
    "CodeChange": {
      "additionalProperties": false,
      "description": "Represents a single code change.",
      "properties": {
        "file_path": {
          "title": "File Path",
          "type": "string"
        },
        "change_type": {
          "title": "Change Type",
          "type": "string"
        },
        "lines_added": {
          "default": 0,
          "title": "Lines Added",
          "type": "integer"
        },
        "lines_removed": {
          "default": 0,
          "title": "Lines Removed",
          "type": "integer"
        },
        "functions_added": {
          "items": {
            "type": "string"
          },
          "title": "Functions Added",
          "type": "array"
        },
        "functions_modified": {
          "items": {
            "type": "string"
          },
          "title": "Functions Modified",
          "type": "array"
        },
        "functions_removed": {
          "items": {
            "type": "string"
          },
          "title": "Functions Removed",
          "type": "array"
        },
        "complexity_before": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Complexity Before"
        },
        "complexity_after": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Complexity After"
        },
        "status": {
          "default": "pending",
          "title": "Status",
          "type": "string"
        },
        "error_message": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Error Message"
        }
      },
      "required": [
        "file_path",
        "change_type"
      ],
      "title": "CodeChange",
      "type": "object"
    },
    "OperationType": {
      "description": "Type of operation performed by an agent.",
      "enum": [
        "plan",
        "create-story",
        "list-stories",
        "design-system",
        "create-diagram",
        "select-technology",
        "design-security",
        "define-boundaries",
        "write-code",
        "refactor",
        "generate-code",
        "review-code",
        "score-code",
        "lint-code",
        "type-check",
        "security-scan",
        "write-tests",
        "run-tests",
        "generate-tests",
        "document",
        "generate-docs",
        "update-readme",
        "document-api",
        "enhance-prompt",
        "enhance-quick",
        "enhance-stage",
        "security-audit",
        "compliance-check",
        "deploy",
        "infrastructure-setup",
        "dependency-audit"
      ],
      "title": "OperationType",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "properties": {
    "schema_version": {
      "default": "1.0",
      "title": "Schema Version",
      "type": "string"
    },
    "timestamp": {
      "title": "Timestamp",
      "type": "string"
    },
    "status": {
      "$ref": "#/$defs/ArtifactStatus",
      "default": "pending"
    },
    "worktree_path": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Worktree Path"
    },
    "correlation_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Correlation Id"
    },
    "operation_type": {
      "anyOf": [
        {
          "$ref": "#/$defs/OperationType"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    },
    "changes": {
      "items": {
        "$ref": "#/$defs/CodeChange"
      },
      "title": "Changes",
      "type": "array"
    },
    "refactorings": {
      "items": {
        "$ref": "#/$defs/CodeChange"
      },
      "title": "Refactorings",
      "type": "array"
    },
    "total_files_modified": {
      "default": 0,
      "title": "Total Files Modified",
      "type": "integer"
    },
    "total_lines_added": {
      "default": 0,
      "title": "Total Lines Added",
      "type": "integer"
    },
    "total_lines_removed": {
      "default": 0,
      "title": "Total Lines Removed",
      "type": "integer"
    },
    "total_functions_added": {
      "default": 0,
      "title": "Total Functions Added",
      "type": "integer"
    },
    "review_passed": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Review Passed"
    },
    "review_score": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Review Score"
    },
    "review_threshold": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Review Threshold"
    },
    "error": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Error"
    },
    "cancelled": {
      "default": false,
      "title": "Cancelled",
      "type": "boolean"
    },
    "execution_time_seconds": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Execution Time Seconds"
    },
    "metadata": {
      "$ref": "#/$defs/ArtifactMetadata"
    }
  },
  "type": "object"
}