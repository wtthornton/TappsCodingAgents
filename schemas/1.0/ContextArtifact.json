{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/ContextArtifact.json",
  "title": "ContextArtifact",
  "description": "Versioned context & knowledge artifact.\n\nSchema version: 1.0\nMigrated to Pydantic BaseModel for runtime validation and type safety.",
  "version": "1.0",
  "$defs": {
    "ArtifactMetadata": {
      "additionalProperties": false,
      "description": "Flexible but typed metadata structure for artifacts.\n\nUses TypedDict with total=False to allow partial metadata while maintaining\ntype hints for known fields.",
      "properties": {
        "agent_version": {
          "title": "Agent Version",
          "type": "string"
        },
        "execution_context": {
          "title": "Execution Context",
          "type": "string"
        },
        "custom_tags": {
          "items": {
            "type": "string"
          },
          "title": "Custom Tags",
          "type": "array"
        },
        "related_artifacts": {
          "items": {
            "type": "string"
          },
          "title": "Related Artifacts",
          "type": "array"
        },
        "workflow_id": {
          "title": "Workflow Id",
          "type": "string"
        },
        "step_id": {
          "title": "Step Id",
          "type": "string"
        },
        "agent_id": {
          "title": "Agent Id",
          "type": "string"
        },
        "execution_mode": {
          "title": "Execution Mode",
          "type": "string"
        }
      },
      "title": "ArtifactMetadata",
      "type": "object"
    },
    "ArtifactStatus": {
      "description": "Status of an artifact during workflow execution.",
      "enum": [
        "pending",
        "running",
        "completed",
        "failed",
        "cancelled",
        "timeout",
        "not_run"
      ],
      "title": "ArtifactStatus",
      "type": "string"
    },
    "ContextQuery": {
      "additionalProperties": false,
      "description": "Context query result.",
      "properties": {
        "query": {
          "title": "Query",
          "type": "string"
        },
        "library": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Library"
        },
        "results_count": {
          "default": 0,
          "title": "Results Count",
          "type": "integer"
        },
        "cache_hit": {
          "default": false,
          "title": "Cache Hit",
          "type": "boolean"
        },
        "execution_time_seconds": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Execution Time Seconds"
        },
        "error_message": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Error Message"
        }
      },
      "required": [
        "query"
      ],
      "title": "ContextQuery",
      "type": "object"
    },
    "LibraryCacheEntry": {
      "additionalProperties": false,
      "description": "Library cache entry information.",
      "properties": {
        "library_name": {
          "title": "Library Name",
          "type": "string"
        },
        "library_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Library Id"
        },
        "status": {
          "default": "pending",
          "title": "Status",
          "type": "string"
        },
        "cache_size_bytes": {
          "default": 0,
          "title": "Cache Size Bytes",
          "type": "integer"
        },
        "cache_hit_count": {
          "default": 0,
          "title": "Cache Hit Count",
          "type": "integer"
        },
        "last_accessed": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Last Accessed"
        },
        "error_message": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Error Message"
        }
      },
      "required": [
        "library_name"
      ],
      "title": "LibraryCacheEntry",
      "type": "object"
    },
    "ProjectProfile": {
      "additionalProperties": false,
      "description": "Project profiling information.",
      "properties": {
        "deployment_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Deployment Type"
        },
        "tenancy": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Tenancy"
        },
        "user_scale": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "User Scale"
        },
        "compliance": {
          "items": {
            "type": "string"
          },
          "title": "Compliance",
          "type": "array"
        },
        "security_posture": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Security Posture"
        },
        "relevant_experts": {
          "items": {
            "type": "string"
          },
          "title": "Relevant Experts",
          "type": "array"
        }
      },
      "title": "ProjectProfile",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "properties": {
    "schema_version": {
      "default": "1.0",
      "title": "Schema Version",
      "type": "string"
    },
    "timestamp": {
      "title": "Timestamp",
      "type": "string"
    },
    "status": {
      "$ref": "#/$defs/ArtifactStatus",
      "default": "pending"
    },
    "worktree_path": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Worktree Path"
    },
    "correlation_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Correlation Id"
    },
    "operation_type": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Operation Type"
    },
    "libraries_cached": {
      "items": {
        "$ref": "#/$defs/LibraryCacheEntry"
      },
      "title": "Libraries Cached",
      "type": "array"
    },
    "cache_population_success": {
      "default": 0,
      "title": "Cache Population Success",
      "type": "integer"
    },
    "cache_population_failed": {
      "default": 0,
      "title": "Cache Population Failed",
      "type": "integer"
    },
    "total_cache_size_bytes": {
      "default": 0,
      "title": "Total Cache Size Bytes",
      "type": "integer"
    },
    "cache_hit_rate": {
      "default": 0.0,
      "title": "Cache Hit Rate",
      "type": "number"
    },
    "queries_executed": {
      "items": {
        "$ref": "#/$defs/ContextQuery"
      },
      "title": "Queries Executed",
      "type": "array"
    },
    "total_queries": {
      "default": 0,
      "title": "Total Queries",
      "type": "integer"
    },
    "cache_hits": {
      "default": 0,
      "title": "Cache Hits",
      "type": "integer"
    },
    "cache_misses": {
      "default": 0,
      "title": "Cache Misses",
      "type": "integer"
    },
    "project_profile": {
      "anyOf": [
        {
          "$ref": "#/$defs/ProjectProfile"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    },
    "cache_cleanup_performed": {
      "default": false,
      "title": "Cache Cleanup Performed",
      "type": "boolean"
    },
    "files_removed": {
      "default": 0,
      "title": "Files Removed",
      "type": "integer"
    },
    "space_freed_bytes": {
      "default": 0,
      "title": "Space Freed Bytes",
      "type": "integer"
    },
    "error": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Error"
    },
    "cancelled": {
      "default": false,
      "title": "Cancelled",
      "type": "boolean"
    },
    "timeout": {
      "default": false,
      "title": "Timeout",
      "type": "boolean"
    },
    "execution_time_seconds": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Execution Time Seconds"
    },
    "metadata": {
      "$ref": "#/$defs/ArtifactMetadata"
    }
  },
  "type": "object"
}