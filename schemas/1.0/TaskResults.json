{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/TaskResults.json",
  "title": "TaskResults",
  "description": "Structured task results for agent task completions.",
  "version": "1.0",
  "additionalProperties": false,
  "properties": {
    "output_files": {
      "items": {
        "type": "string"
      },
      "title": "Output Files",
      "type": "array"
    },
    "metrics": {
      "additionalProperties": {
        "type": "number"
      },
      "title": "Metrics",
      "type": "object"
    },
    "artifacts": {
      "items": {
        "type": "string"
      },
      "title": "Artifacts",
      "type": "array"
    },
    "errors": {
      "items": {
        "type": "string"
      },
      "title": "Errors",
      "type": "array"
    },
    "warnings": {
      "items": {
        "type": "string"
      },
      "title": "Warnings",
      "type": "array"
    },
    "output": {
      "additionalProperties": true,
      "title": "Output",
      "type": "object"
    }
  },
  "type": "object"
}