{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/QualityArtifact.json",
  "title": "QualityArtifact",
  "description": "Versioned quality analysis artifact.\n\nSchema version: 1.0\nMigrated to Pydantic BaseModel for runtime validation and type safety.",
  "version": "1.0",
  "$defs": {
    "ArtifactMetadata": {
      "additionalProperties": false,
      "description": "Flexible but typed metadata structure for artifacts.\n\nUses TypedDict with total=False to allow partial metadata while maintaining\ntype hints for known fields.",
      "properties": {
        "agent_version": {
          "title": "Agent Version",
          "type": "string"
        },
        "execution_context": {
          "title": "Execution Context",
          "type": "string"
        },
        "custom_tags": {
          "items": {
            "type": "string"
          },
          "title": "Custom Tags",
          "type": "array"
        },
        "related_artifacts": {
          "items": {
            "type": "string"
          },
          "title": "Related Artifacts",
          "type": "array"
        },
        "workflow_id": {
          "title": "Workflow Id",
          "type": "string"
        },
        "step_id": {
          "title": "Step Id",
          "type": "string"
        },
        "agent_id": {
          "title": "Agent Id",
          "type": "string"
        },
        "execution_mode": {
          "title": "Execution Mode",
          "type": "string"
        }
      },
      "title": "ArtifactMetadata",
      "type": "object"
    },
    "ArtifactStatus": {
      "description": "Status of an artifact during workflow execution.",
      "enum": [
        "pending",
        "running",
        "completed",
        "failed",
        "cancelled",
        "timeout",
        "not_run"
      ],
      "title": "ArtifactStatus",
      "type": "string"
    },
    "ToolResult": {
      "additionalProperties": false,
      "description": "Result from a single quality tool (Ruff, mypy, etc.).",
      "properties": {
        "tool_name": {
          "title": "Tool Name",
          "type": "string"
        },
        "available": {
          "title": "Available",
          "type": "boolean"
        },
        "status": {
          "title": "Status",
          "type": "string"
        },
        "issues": {
          "items": {
            "additionalProperties": true,
            "type": "object"
          },
          "title": "Issues",
          "type": "array"
        },
        "issue_count": {
          "default": 0,
          "title": "Issue Count",
          "type": "integer"
        },
        "error_count": {
          "default": 0,
          "title": "Error Count",
          "type": "integer"
        },
        "warning_count": {
          "default": 0,
          "title": "Warning Count",
          "type": "integer"
        },
        "error_message": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Error Message"
        },
        "execution_time_seconds": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Execution Time Seconds"
        }
      },
      "required": [
        "tool_name",
        "available",
        "status"
      ],
      "title": "ToolResult",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "properties": {
    "schema_version": {
      "default": "1.0",
      "title": "Schema Version",
      "type": "string"
    },
    "timestamp": {
      "title": "Timestamp",
      "type": "string"
    },
    "status": {
      "$ref": "#/$defs/ArtifactStatus",
      "default": "pending"
    },
    "worktree_path": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Worktree Path"
    },
    "correlation_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Correlation Id"
    },
    "tools": {
      "additionalProperties": {
        "$ref": "#/$defs/ToolResult"
      },
      "title": "Tools",
      "type": "object"
    },
    "scores": {
      "additionalProperties": {
        "type": "number"
      },
      "title": "Scores",
      "type": "object"
    },
    "total_issues": {
      "default": 0,
      "title": "Total Issues",
      "type": "integer"
    },
    "total_errors": {
      "default": 0,
      "title": "Total Errors",
      "type": "integer"
    },
    "total_warnings": {
      "default": 0,
      "title": "Total Warnings",
      "type": "integer"
    },
    "overall_score": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Overall Score"
    },
    "error": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Error"
    },
    "cancelled": {
      "default": false,
      "title": "Cancelled",
      "type": "boolean"
    },
    "timeout": {
      "default": false,
      "title": "Timeout",
      "type": "boolean"
    },
    "execution_time_seconds": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Execution Time Seconds"
    },
    "metadata": {
      "$ref": "#/$defs/ArtifactMetadata"
    }
  },
  "type": "object"
}