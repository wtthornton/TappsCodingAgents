{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/TestingArtifact.json",
  "title": "TestingArtifact",
  "description": "Versioned testing analysis artifact.\n\nSchema version: 1.0\nMigrated to Pydantic BaseModel for runtime validation and type safety.",
  "version": "1.0",
  "$defs": {
    "ArtifactMetadata": {
      "additionalProperties": false,
      "description": "Flexible but typed metadata structure for artifacts.\n\nUses TypedDict with total=False to allow partial metadata while maintaining\ntype hints for known fields.",
      "properties": {
        "agent_version": {
          "title": "Agent Version",
          "type": "string"
        },
        "execution_context": {
          "title": "Execution Context",
          "type": "string"
        },
        "custom_tags": {
          "items": {
            "type": "string"
          },
          "title": "Custom Tags",
          "type": "array"
        },
        "related_artifacts": {
          "items": {
            "type": "string"
          },
          "title": "Related Artifacts",
          "type": "array"
        },
        "workflow_id": {
          "title": "Workflow Id",
          "type": "string"
        },
        "step_id": {
          "title": "Step Id",
          "type": "string"
        },
        "agent_id": {
          "title": "Agent Id",
          "type": "string"
        },
        "execution_mode": {
          "title": "Execution Mode",
          "type": "string"
        }
      },
      "title": "ArtifactMetadata",
      "type": "object"
    },
    "ArtifactStatus": {
      "description": "Status of an artifact during workflow execution.",
      "enum": [
        "pending",
        "running",
        "completed",
        "failed",
        "cancelled",
        "timeout",
        "not_run"
      ],
      "title": "ArtifactStatus",
      "type": "string"
    },
    "CoverageSummary": {
      "additionalProperties": false,
      "description": "Test coverage summary.",
      "properties": {
        "total_lines": {
          "default": 0,
          "title": "Total Lines",
          "type": "integer"
        },
        "covered_lines": {
          "default": 0,
          "title": "Covered Lines",
          "type": "integer"
        },
        "coverage_percentage": {
          "default": 0.0,
          "title": "Coverage Percentage",
          "type": "number"
        },
        "missing_lines": {
          "items": {
            "type": "integer"
          },
          "title": "Missing Lines",
          "type": "array"
        },
        "branch_coverage": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Branch Coverage"
        },
        "statement_coverage": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Statement Coverage"
        }
      },
      "title": "CoverageSummary",
      "type": "object"
    },
    "TestResult": {
      "additionalProperties": false,
      "description": "Result from test execution.",
      "properties": {
        "test_name": {
          "title": "Test Name",
          "type": "string"
        },
        "status": {
          "title": "Status",
          "type": "string"
        },
        "duration_seconds": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Duration Seconds"
        },
        "error_message": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Error Message"
        },
        "stdout": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Stdout"
        },
        "stderr": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Stderr"
        }
      },
      "required": [
        "test_name",
        "status"
      ],
      "title": "TestResult",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "properties": {
    "schema_version": {
      "default": "1.0",
      "title": "Schema Version",
      "type": "string"
    },
    "timestamp": {
      "title": "Timestamp",
      "type": "string"
    },
    "status": {
      "$ref": "#/$defs/ArtifactStatus",
      "default": "pending"
    },
    "worktree_path": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Worktree Path"
    },
    "correlation_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Correlation Id"
    },
    "tests_run": {
      "default": false,
      "title": "Tests Run",
      "type": "boolean"
    },
    "test_framework": {
      "default": "pytest",
      "title": "Test Framework",
      "type": "string"
    },
    "total_tests": {
      "default": 0,
      "title": "Total Tests",
      "type": "integer"
    },
    "passed_tests": {
      "default": 0,
      "title": "Passed Tests",
      "type": "integer"
    },
    "failed_tests": {
      "default": 0,
      "title": "Failed Tests",
      "type": "integer"
    },
    "skipped_tests": {
      "default": 0,
      "title": "Skipped Tests",
      "type": "integer"
    },
    "error_tests": {
      "default": 0,
      "title": "Error Tests",
      "type": "integer"
    },
    "success_rate": {
      "default": 0.0,
      "title": "Success Rate",
      "type": "number"
    },
    "test_results": {
      "items": {
        "$ref": "#/$defs/TestResult"
      },
      "title": "Test Results",
      "type": "array"
    },
    "coverage_enabled": {
      "default": false,
      "title": "Coverage Enabled",
      "type": "boolean"
    },
    "coverage": {
      "anyOf": [
        {
          "$ref": "#/$defs/CoverageSummary"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    },
    "execution_time_seconds": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Execution Time Seconds"
    },
    "return_code": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Return Code"
    },
    "stdout": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Stdout"
    },
    "stderr": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Stderr"
    },
    "error": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Error"
    },
    "cancelled": {
      "default": false,
      "title": "Cancelled",
      "type": "boolean"
    },
    "timeout": {
      "default": false,
      "title": "Timeout",
      "type": "boolean"
    },
    "not_run_reason": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Not Run Reason"
    },
    "metadata": {
      "$ref": "#/$defs/ArtifactMetadata"
    }
  },
  "type": "object"
}