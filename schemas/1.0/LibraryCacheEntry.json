{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tapps-coding-agents.com/schemas/1.0/LibraryCacheEntry.json",
  "title": "LibraryCacheEntry",
  "description": "Library cache entry information.",
  "version": "1.0",
  "additionalProperties": false,
  "properties": {
    "library_name": {
      "title": "Library Name",
      "type": "string"
    },
    "library_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Library Id"
    },
    "status": {
      "default": "pending",
      "title": "Status",
      "type": "string"
    },
    "cache_size_bytes": {
      "default": 0,
      "title": "Cache Size Bytes",
      "type": "integer"
    },
    "cache_hit_count": {
      "default": 0,
      "title": "Cache Hit Count",
      "type": "integer"
    },
    "last_accessed": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Last Accessed"
    },
    "error_message": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Error Message"
    }
  },
  "required": [
    "library_name"
  ],
  "type": "object"
}